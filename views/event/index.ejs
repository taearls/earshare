	<h3>Events</h3><br>
		<div class="row">
			<div class="col-sm-3">
				<ul>
					<%for (let i = 0; i < events.length; i++) {%>
						<li><p><a href="/event/<%=events[i].id%>"><%=events[i].name%></a></p><br />
						<img src="<%=events[i].img%>" class="img-responsive" style="width:100%" alt="<%=events[i].name%> Image"></li>
					<%}%>
				</ul>
			</div>

		</div>
	</div>

	<footer class="container-fluid text-center">
		<p>Footer Here</p>
	</footer>


